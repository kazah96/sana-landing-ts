{
  "name": "gatsby-starter-default",
  "private": true,
  "description": "A simple starter to get up and developing quickly with Gatsby",
  "version": "0.1.0",
  "author": "Kyle Mathews <mathews.kyle@gmail.com>",
  "dependencies": {
    "classnames": "^2.2.6",
    "gatsby": "^2.24.26",
    "gatsby-image": "^2.4.14",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.1.0",
    "react-markdown": "^4.3.1",
    "react-media": "^1.10.0"
  },
  "devDependencies": {
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "gatsby-plugin-manifest": "^2.2.39",
    "gatsby-plugin-offline": "^3.0.32",
    "gatsby-plugin-react-helmet": "^3.1.21",
    "gatsby-plugin-robots-txt": "^1.5.1",
    "gatsby-plugin-sharp": "^2.4.5",
    "gatsby-source-filesystem": "^2.1.46",
    "gatsby-source-strapi": "^0.0.10",
    "gatsby-transformer-sharp": "^2.3.14",
    "prettier": "^1.19.1",
    "surge": "^0.21.6"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "convert": "node ./convert.js files",
    "deploy:main": "surge --project ./public --domain https://umirzakovs.surge.sh ",
    "deploy:test": "surge --project ./public --domain https://umirzakovs-test.surge.sh ",
    "deploy:prod": "surge --project ./public --domain https://umirzakov.site",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/gatsbyjs/gatsby-starter-default"
  },
  "bugs": {
    "url": "https://github.com/gatsbyjs/gatsby/issues"
  }
}
